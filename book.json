{
    "title": "gintangible 的文档库",
    "author": "gintangible",
    "description": "gintangible 的文档库. ",
    "language": "zh-hans",
    "styles": {},
    "plugins": [
        "-search",
        "-sharing",
        "-highlight",
        "search-pro",
        "baidu-tongji",
        "expandable-chapters",
        "github-buttons",
        "splitter",
        "copy-code-button",
        "insert-logo",
        "favicon"
    ],
    "pluginsConfig": {
        "prism": {
            "css": [
                "prismjs/themes/prism-solarizedlight.css"
            ],
            "lang": {
                "shell": "bash"
            }
        },

        "github-buttons": {
            "buttons": [{
                    "user": "Gintangible",
                    "repo": "document-library",
                    "type": "star",
                    "count": true,
                    "size": "small"
                },
                {
                    "user": "Gintangible",
                    "width": "160",
                    "type": "follow",
                    "count": true,
                    "size": "small"
                }
            ]
        },
        "insert-logo": {
            "url": "https://github.com/Gintangible/document-library/assets/images/logo.png",
            "style": "background: none; height: 60px;"
        },
        "favicon": {
            "shortcut": "https://github.com/Gintangible/document-library/favicon.ico"
        },
        "baidu-tongji": {
            "token": ""
        }
    }
}