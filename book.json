    "plugins": [
        "theme-comscore",
        "prism",
        "-highlight",
        "copy-code-button",
        "search-pro",
        "-search",
        "-lunr",
        "expandable-chapters",
        "splitter",
        "-sharing",
        "github-buttons",
        "donate",
        "tbfed-pagefooter",
        "baidu-tongji",
        "anchor-navigation-ex"
